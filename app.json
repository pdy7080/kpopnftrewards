{
  "expo": {
    "name": "KpopNftRewards",
    "slug": "KpopNftRewards",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/709fcbf4-1766-4110-ae2a-14af3b067b0b"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dcclab.KpopNftRewards",
      "infoPlist": {
        "NSCameraUsageDescription": "QR 코드를 스캔하기 위해 카메라 접근 권한이 필요합니다."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dcclab.KpopNftRewards",
      "versionName": "1.0.2",
      "permissions": ["CAMERA"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "QR 코드를 스캔하기 위해 카메라 접근 권한이 필요합니다."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "709fcbf4-1766-4110-ae2a-14af3b067b0b"
      }
    },
    "developmentClient": {
      "silentLaunch": true
    },
    "development": {
      "developmentClient": true
    }
  }
}
